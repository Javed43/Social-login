 <%= form_for @user do |f| %>

  <p>
  <%= f.label :username %>
  <%= f.text_field :username %><br /> 
  </p>

  <p>
  <%= f.label :email %>
  <%= f.text_field :email %><br /> 
  </p>

  <p>
  <%= f.label :password %>
  <%= f.password_field :password %><br />
  </p>
  <%= f.submit "SignUp"%>

  <% end %>